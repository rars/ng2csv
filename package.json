{
  "name": "ng2csv",
  "version": "0.4.0",
  "description": "Angular2 module for saving CSV files.",
  "main": "Ng2Csv.bundle.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run lint && tsc -p tsconfig-esm.json && rollup -c rollup.config.js dist/index.js > dist/Ng2Csv.bundle.js && cp package.json dist && ngc && cp README.md dist && cp LICENSE.md dist",
    "test": "karma start --single-run",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "release": "standard-version",
    "npmpublish": "cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rars/ng2csv"
  },
  "keywords": [
    "Angular2",
    "ng2",
    "CSV"
  ],
  "author": "Richard Russell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rars/ng2csv/issues"
  },
  "homepage": "https://github.com/rars/ng2csv#readme",
  "peerDependencies": {
    "@angular/core": ">=2.0.0",
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/compiler-cli": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/platform-server": "^2.4.10",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@types/file-saver": "0.0.1",
    "@types/jasmine": "^2.5.53",
    "@types/jsdom": "^11.0.1",
    "@types/node": "^8.0.13",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^2.1.0",
    "file-saver": "^1.3.3",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.4",
    "jsdom": "^9.12.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "rollup": "^0.45.1",
    "rxjs": "^5.4.2",
    "standard-version": "^4.3.0",
    "ts-loader": "^2.2.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "zone.js": "^0.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
