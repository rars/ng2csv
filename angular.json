{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng2csv": {
      "projectType": "library",
      "root": "projects/ng2csv",
      "sourceRoot": "projects/ng2csv/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ng2csv/tsconfig.lib.json",
            "project": "projects/ng2csv/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng2csv/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng2csv/src/test.ts",
            "tsConfig": "projects/ng2csv/tsconfig.spec.json",
            "karmaConfig": "projects/ng2csv/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ng2csv/**/*.ts",
              "projects/ng2csv/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "defaultProject": "ng2csv"
}
